{
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/:path*\\.(avif|webp|jpg|jpeg|png|svg|gif|mp4|webm)",
      "headers": [
        { "key": "cache-control", "value": "public, max-age=31536000, immutable" }
      ]
    },
    {
      "source": "/:path*.html",
      "headers": [
        { "key": "cache-control", "value": "public, max-age=0, must-revalidate" }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "www.several.club" }],
      "destination": "https://several.club/:path*",
      "permanent": true
    }
  ]
}


